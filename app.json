{
  "expo": {
    "name": "day-planner",
    "slug": "day-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/add.png",
    "scheme": "dayplanner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ando.dayplanner"
    },
    "android": {
      "package": "com.ando.dayplanner"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
